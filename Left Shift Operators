import java.util.Scanner;

class Test {
    public static void main(String[] args) {
        int x = 3;

        System.out.println(x << 1);
        System.out.println(x << 2);

        int y = 4;
        int z = (x << y);
        System.out.println(z);

        int a = -1;
        System.out.println(a << 1);
    }
}
