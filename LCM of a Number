// User function Template for Java
class Solution {
    public static int LCM(int a, int b) {

        // write your code here
        int ans = 1;
        for(int i = a;i<=a*b;i=i+a)
        {
            if((i%b)==0){
                ans = i;
                break;
            }
        }
        return ans;

        // return LCM of a and b
    }
}
